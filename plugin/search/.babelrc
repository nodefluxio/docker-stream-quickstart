{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      ["module-resolver", {
        "alias": {
          "api": "./src/api",
          "assets": "./src/assets",
          "components": "./src/components",
          "config": "./src/config",
          "helpers": "./src/helpers",
          "theme": "./src/theme",
          "router": "./src/router"
      }
      }],
      "styled-components"
    ]
  }